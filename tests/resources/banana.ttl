@prefix ex: <http://example.org/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

ex:Banana rdf:type ex:Fruit ;
          ex:hasColor "yellow" ;
          ex:hasTaste "sweet" ;
          ex:hasShape "curved" ;
          ex:hasWeight "120 grams" ;
          ex:growsIn ex:TropicalClimate ;
          ex:isEdible true ;
          ex:hasVitamin ex:VitaminB6 ;
          ex:hasPrice "$0.50" ;
          ex:hasOrigin "Ecuador" .

ex:Fruit rdf:type rdfs:Class;
          rdfs:label "Fruit".